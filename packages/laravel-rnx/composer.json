{
  "name": "arnelirobles/laravel-rnx",
  "description": "rnxJS integration for Laravel - Blade directives and helpers for reactive components",
  "type": "library",
  "license": "MPL-2.0",
  "authors": [
    {
      "name": "Arnel Irobles",
      "email": "arnel@arnelirobles.com"
    }
  ],
  "keywords": [
    "laravel",
    "rnxjs",
    "reactive",
    "components",
    "blade"
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0",
    "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "ArnelIrobles\\RnxLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArnelIrobles\\RnxLaravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArnelIrobles\\RnxLaravel\\RnxServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
